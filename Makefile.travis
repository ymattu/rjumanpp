install:
	# JUMAN++
	mkdir tmp &&\
	wget http://lotus.kuee.kyoto-u.ac.jp/nl-resource/jumanpp/jumanpp-$(JVER).tar.xz -O /tmp/jumanpp.tar.xz &&\
	tar xJvf /tmp/jumanpp.tar.xz -C /tmp &&\
	cd /tmp/jumanpp-$(JVER) &&\
	./configure --prefix=/usr/local/ && make && make install &&\
  rm -rf /tmp/* &&\
  rm -rf /var/cache/apk/* &&\
